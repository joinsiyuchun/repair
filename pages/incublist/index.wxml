<view class="main">

  <view class="art-title">
    {{incub.type}}
  </view>
  <view class="art-content">
    <view> 状态： {{incub.status}}</view>
    <view> 房间： {{incub.room}}</view>
    <view> 序列号：{{incub.seriesno}}</view>
     <view> P/N：{{incub.pn}}</view>
    <view class='tips' bindtap='qcorder' data-id='{{incub.id}}'>质控检查</view>
  </view>
  <view class="content" style="height:{{winHeight}}px">
    <block wx:for="{{incublist}}" wx:for-item="item" >
      <view class="bg">
        <view class="item">
          <view class="wrapper left">
            <view class="normal">类型</view>
            <view class="line"></view>
            <view class="blue">{{item.type}}</view>
          </view>
          <view class="wrapper center">
            <view class="normal">执行人</view>
            <view class="line"></view>
            <view class="small">{{item.operator_name}}</view>
          </view>
          <view class="wrapper center">
            <view class="normal">执行日期</view>
            <view class="line"></view>
            <view class="small">{{item.created_dt}}</view>
          </view>
          <view class="wrapper right">
            <view class="blue"></view>
            <view class="buy">{{item.result}}</view>
          </view>
        </view>
        <view class="hr"></view>
        <view class="seat">
          备注：{{item.memo}}
        </view>
      </view>
    </block>
   <view class="bottomNav">
     <view id="0" class="{{currentTab==0?'selected':'common'}}" bindtap="switchNav">全部</view>
     <view style="color:#ffffff">|</view>
     <view id="1" class="{{currentTab==1?'selected':'common'}}" bindtap="switchNav">每日检查</view>
     <view style="color:#ffffff">|</view>
     <view id="2" class="{{currentTab==2?'selected':'common'}}" bindtap="switchNav">每周检查</view>
     <view style="color:#ffffff">|</view>
     <view id="3" class="{{currentTab==3?'selected':'common'}}" bindtap="switchNav">预防性维护</view>
  </view>
  </view>
   
</view>